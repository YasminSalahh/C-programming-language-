#include <stdio.h>
#include <stdlib.h>
int main()
{
    float money_in_USD, money_in_EGP;
    printf("Enter your money in USD : ");
    scanf("%f", &money_in_USD);
    printf("Your money in USD = %0.3f $\n", money_in_USD);
    money_in_EGP = money_in_USD * 16.65;
    printf("Your money in EGP = %0.3f LE", money_in_EGP);
    return 0;
}

